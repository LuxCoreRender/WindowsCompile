<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <LUX_QT_ROOT>$(LUX_X64_QT_ROOT)</LUX_QT_ROOT>
    <LUX_OCL_LIBS>$(LUX_X64_OCL_LIBS)</LUX_OCL_LIBS>
    <LUX_OCL_INCLUDE>$(LUX_X64_OCL_INCLUDE)</LUX_OCL_INCLUDE>
    <LUX_BOOST_ROOT>$(LUX_X64_BOOST_ROOT)</LUX_BOOST_ROOT>
    <LUX_DEPENDENCIES_ROOT>$(LUX_WINDOWS_DEPS_ROOT)\x64\Release</LUX_DEPENDENCIES_ROOT>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>Lux-64bit-Variables</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="LUX_QT_ROOT">
      <Value>$(LUX_QT_ROOT)</Value>
    </BuildMacro>
    <BuildMacro Include="LUX_OCL_LIBS">
      <Value>$(LUX_OCL_LIBS)</Value>
    </BuildMacro>
    <BuildMacro Include="LUX_OCL_INCLUDE">
      <Value>$(LUX_OCL_INCLUDE)</Value>
    </BuildMacro>
    <BuildMacro Include="LUX_BOOST_ROOT">
      <Value>$(LUX_BOOST_ROOT)</Value>
    </BuildMacro>
    <BuildMacro Include="LUX_DEPENDENCIES_ROOT">
      <Value>$(LUX_DEPENDENCIES_ROOT)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>